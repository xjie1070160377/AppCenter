
<% for(entry in mapData){ %>

<label class="checkbox-inline i-checks ${fieldName}">
<input type="radio" name="${fieldName}" id="${fieldName}_${entry.value}" value="${entry.value}" ${entry.value==defaultValue?"checked":""} <% if (isNotEmpty(disabled) ){ %>disabled="${disabled}"<%}%>> ${entry.key} 
</label>

<% } %>

